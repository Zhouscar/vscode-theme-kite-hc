                "entity.name.tag",
                "keyword",
                "keyword.operator",
                "storage.modifier",
                "storage.type",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#fc6e86"
            }
        },
        {
            "name": "Language Variable",
            "scope": [
                "variable.language",
                "fenced_code.block.language"
