    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#8a9eb3"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation"
            ],
            "settings": {
                "foreground": "#a9b4c1"
            }
        },
        {
            "scope": [
                "punctuation.decorator",
                "meta.brace.round"
            ],
            "settings": {
                "foreground": "#c7ced6"
            }
        },
        {
            "scope": [
                "punctuation.terminator.statement"
            ],
            "settings": {
                "foreground": "#8f9dae"
            }
        },
        {
            "scope": [
                "punctuation.accessor.optional"
            ],
            "settings": {
                "foreground": "#bb7fff"
            }
        },
        {
            "scope": [
                "punctuation.definition.constant"
            ],
            "settings": {
                "foreground": "#6eb0fc"
            }
        },
        {
            "scope": [
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#c87ded"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#edd37d"
